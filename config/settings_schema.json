[
  {
    "name": "theme_info",
    "theme_name": "Cornerstone theme",
    "theme_version": "1.0.0",
    "theme_author": "Kajabi",
    "theme_documentation_url": "",
    "theme_support_url": "https://github.com/Kajabi/theme-cornerstone-page/issues"
  },
  {
    "name": "Favicon",
    "elements": [
      {
        "type": "header",
        "content": "Favicon"
      },
      {
        "type": "image_picker",
        "id": "favicon",
        "label": "Favicon Image",
        "fit": "crop",
        "width": 32,
        "height": 32,
        "info": "Must be a 32x32 PNG file. Transparency is supported. Please allow some time for favicon to change. Learn how to create your own favicon [here](canva_logos)"
      }
    ]
  },
  {
    "name": "Typography",
    "elements": [
      {
        "type": "header",
        "content": "Typography"
      },
      {
        "type": "font_select",
        "label": "Font Family",
        "id": "font_family",
        "default": "Open Sans",
        "preset": "google_fonts",
        "options": []
      }
    ]
  },
  {
    "name": "Color Scheme",
    "elements": [
      {
        "type": "header",
        "content": "Color Scheme"
      },
      {
        "type": "color",
        "id": "color_background",
        "label": "Page Background",
        "default": "#f8f9fd"
      },
      {
        "type": "color",
        "id": "color_text",
        "label": "Text",
        "default": "#161E2A"
      }
    ]
  }
]
